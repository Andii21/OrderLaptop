@model IEnumerable<OrderLaptop.Models.DeviceViewModel.OrderGroup>
  
@{
    ViewData["Title"] = "Ordered Laptops Statistics";
}
<h2>Ordered Laptops Statistics</h2>
<p> Numarul de laptopuri comandate pentru fiecare data. </p>
<table>
    <tr>
        <th>
            Order Date
        </th>
        <th>
            
        </th>
        <th></th>
        <th>
            Ordered Laptops
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                 @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            
            
            <td>
                @item.LaptopCount
            </td>
        </tr>
    }
</table>